{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Census table schema",
  "description": "",
  "type": "array",
  
  "items": {
    "type": "object",
    "properties": {    

      "geo_level": {
        "type": "string",
        "description": "Geographic level `0=national`, `1=state`, `2=district`, `3=sub-district`, `4=town/village`, `5=ward`"
      },

      "state": {
        "type": "string",
        "description": "State code "
      },

      "district": {
        "type": "string",
        "description": "District code "
      },

      "subdistrict": {
        "type": "string",
        "description": "Subdistrict code "
      },

      "town": {
        "type": "string",
        "description": "Town code "
      },

      "block": {
        "type": "string",
        "description": "Block code "
      },

      "value": {
        "type": "string",
        "description": "Value"
      },

      "indicator": {
        "type": "integer",
        "description": "Indicator"
      }
      
  },
  "required": [
    "geo_level","indicator","value"
  ]
}
}
